<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cl.ido.ejemplo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="cl.ido.gastos.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="cl.ido.gastos.AddGastoActivity"
            android:label="@string/title_activity_add_gasto" >
        </activity>
        <activity
            android:name="cl.ido.gastos.DeleteAllActivity"
            android:label="@string/title_activity_delete_all" >
        </activity>
        <activity
            android:name="cl.ido.gastos.GastosBySubCategoriaActivity"
            android:label="@string/title_activity_gastos_by_sub_categoria" >
        </activity>
        <activity
            android:name="cl.ido.gastos.AddCategoriaActivity"
            android:label="@string/title_activity_add_categoria" >
        </activity>
        <activity
            android:name="cl.ido.gastos.AddSubCategoriaActivity"
            android:label="@string/title_activity_add_sub_categoria" >
        </activity>
    </application>

</manifest>
